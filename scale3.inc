;*********************** CODE SEG DATA **********************************

.code
Srcsize         dw  100     ;square 
IntScale        dw  0
Fraction        dw  0
x16count        dw  0       ;inc. count x16 (shl 4)
Lastcount       dw  0       ;value of count last
x16scale        dw  0       ;scale x 16
LineInc         dw  0       ;inc to go one line down
SrcLineStart        dw  0       ;Start of source line (for SI)
SrcLinePos      dw  0       ;Place scale tab for line
ScreenWidth     dw  320
LineTabPos      dw  0       ;table pos saver
ScaleStart      dw  0
OffFT           dw  0
;**************************** PROCEDURES! *******************************

TabCopy     MACRO
    mov si, CS:[ScaleStart]
    mov cx, 20
    rep movsb
        ENDM

Finish  MACRO
    mov ax, 4c00h
    int 21h
    ENDM

ClearBuffer PROC NEAR

    mov ax, [SPicSeg]
    mov es, ax
    xor eax, eax
    xor edi, edi
    mov cx, 16000
    rep stosd           ;clear out the buffer

    ret
ClearBuffer     ENDP


WriteScaled PROC NEAR
    push    ds
    
        
    
    mov ax, [SPicSeg]
    mov ds, ax
    mov ax, 0a000h
    mov es, ax
    mov si, 0       ;start of allocated seg
    xor di, di
    mov ax, [LineInc]   ;get distance from edge
    shr ax, 1       ; divide by 2 (centre picture)
    add di, ax      ;add it to di
    shr ax, 1       ;divide by 2 again
    

    
    mov cx, 16000
    rep     movsd
    
    pop ds
    ret
WriteScaled ENDP


DirectScale PROC NEAR
        
    mov ax, CS:[Fraction]
    cmp ax, 0               ;is remainder 0?
    mov CS:[ScaleStart], offset ScaleTabs       ;set up start incase
    je  nofraction2             ;no fraction! 8-)
    
    mov cx, 100             ;for mult       
    mul cx              ;20 lines of table ppn

    mov bx, offset ScaleTabs        ;Start of the tables
    add bx, ax              ;add the offset from above
    mov CS:[ScaleStart], bx
        

nofraction2:
    mov ax, [SPicSeg]           ;set up screen seg
    mov es, ax
    xor di, di              ;DI=0 i.e. start of seg
        
    mov si, offset PicBuffer        ;SI= start of source
                        ;DS already OK  
    
    mov ax, 320             ;set up variable to inc di
    mov cx, CS:[Srcsize]
    mov bx, CS:[ScaleStart]
    xor dx, dx
 countup2:                  
    mov dl, [bx]            ;find out the difference
    sub ax, dx              ;between 320 and the no of
    sub ax, CS:[IntScale]
    inc bx              ;pixels written by FinalTab
    loop    countup2        
    mov CS:[LineInc], ax        ;put the result in mem  
        
    mov cx, CS:[Srcsize]        ;No of pixels across source
    xor ax, ax
    mov bx, CS:[ScaleStart]     ;pointer to scale table
    
    
srclineloop2:
    push    cx              ;save line count
    xor cx, cx

    mov CS:[LineTabPos], bx     ;save source line BX
    mov CS:[SrcLineStart], si       ;save start of source line pos
    mov cl, byte ptr [bx]       ;get no of dupl. from table
    cmp cx, 0
    je  noline2             ;skip if no line here
    call    ScalerLoop
noline2:    
    
    mov cx, CS:[IntScale]       ;get no of dupl. from table
    cmp cx, 0
    je  noline3             ;skip if no line here
    call    ScalerLoop
noline3:    
    add si, CS:[Srcsize]        ;line down in source
    pop cx
    mov bx, CS:[LineTabPos]     ;get table pos for lines back
    inc bx              ;increase it


loop    srclineloop2                
        
    
    ret
DirectScale ENDP

ScalerLoop  PROC NEAR
    deslineloop2:
        push    cx              ;save dest. line count
        mov bx, CS:[ScaleStart]
        mov cx, CS:[Srcsize]        ;no of pixels per line
        pixloop2:
            mov dx, cx              ;save cx in dx
            mov al, [si]            ;get element from si
            inc si              ;SI ++
            xor cx, cx
            mov cl, byte ptr [bx]       ;get pixel duplication
            rep stosb               ;save in DI [bx] times
            mov cx, CS:[IntScale]
            rep stosb
            inc bx              ;next table element
            mov cx, dx              ;restore cx
        loop    pixloop2        
    
        add di, CS:[LineInc]        ;line down on destination
        mov si, CS:[SrcLineStart]       ;put SI back to line start
        pop cx
    loop    deslineloop2
    ret
ScalerLoop  ENDP



OneZoom PROC NEAR
keepitup:

    mov CS:[Intscale], 0
    mov CS:[Fraction], 0        
    mov cx, 50
fractloop:
    push    cx
    PollSB
    call    DirectScale
    call    WriteScaled
    inc CS:[Fraction]
    pop cx
    loop    fractloop
    inc CS:[IntScale]
    mov CS:[Fraction], 0        

    mov cx, 30
fractloop2:
    push    cx
    PollSB
    call    DirectScale
    call    WriteScaled
    inc CS:[Fraction]
    pop cx
    loop    fractloop2

    mov cx, 100
PauseLoop:
    call    WaitRetrace
    PollSB
    loop    PauseLoop

    mov cx, 20          ;the rest for the fade out
bitloop2:
    push    cx
    PollSB
    call    DirectScale
    call    WriteScaled
    inc CS:[Fraction]
    mov bx, 2
    mov si, offset BPalette
    call    DecPalette
    
    pop cx
    loop    bitloop2


    call    ClearBuffer
    call    ClearScreen
    ret
OneZoom     ENDP



ScaleDemo   PROC NEAR

    mov bx, 64000 / 16      ;memory for screen buffer
    call    GetMem
    mov [SPicSeg], ax
    
            
    call    Set13h
    call    ClearScreen     ;flush out the crap in the VGA card
    call    ClearBuffer
    PollSB              ;muzak!
    
        
    mov ax, ds          ;seggie of gfx bufer
    mov di, offset PicBuffer
    mov dx, offset SDatName1
    call    LoadData
    
    PollSB
    mov dx, offset SPalName1
    mov cx, offset BPalette
    call    LoadPal
    
    mov si, offset BPalette
    call    SetPal
    call    OneZoom                         


    mov ax, ds          ;seggie of gfx bufer
    mov di, offset PicBuffer
    mov dx, offset SDatName2
    call    LoadData
    
    PollSB
    mov dx, offset SPalName2
    mov cx, offset BPalette
    call    LoadPal
    
    mov si, offset BPalette
    call    SetPal
    call    OneZoom

    mov ax, ds          ;seggie of gfx bufer
    mov di, offset PicBuffer
    mov dx, offset SDatName3
    call    LoadData
    
    PollSB
    mov dx, offset SPalName3
    mov cx, offset BPalette
    call    LoadPal
    
    mov si, offset BPalette
    call    SetPal
    call    OneZoom
    
        
    mov ax, [SPicSeg]
    mov es, ax
    call    FreeMem

    ret 
ScaleDemo   ENDP